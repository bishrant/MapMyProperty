{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "standard"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "prefer-const": 1,
        "no-var": 1,
        "semi": 0,
        "camelcase": 0,
        "no-useless-constructor": 0,
        "no-unused-vars":"warn"
    }
}
